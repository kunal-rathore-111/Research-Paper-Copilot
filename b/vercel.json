{
    "version": 2,
    "functions": {
        "src/index.js": {
            "maxDuration": 60,
            "memory": 1024
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/src/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}