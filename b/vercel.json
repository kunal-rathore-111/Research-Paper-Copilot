{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ],
    "functions": {
        "src/index.js": {
            "runtime": "nodejs20.x",
            "memory": 1024,
            "maxDuration": 60
        }
    }
}